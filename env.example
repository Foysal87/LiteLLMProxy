# LiteLLM Master Key - used for admin access (must start with 'sk-')
LITELLM_MASTER_KEY=sk-your-master-key-here

# LiteLLM Salt Key - used for encryption/decryption of API keys (cannot be changed after setup)
LITELLM_SALT_KEY=sk-your-salt-key-here

# PostgreSQL Database Configuration
POSTGRES_USER=your_db_user
POSTGRES_PASSWORD=your_db_password
POSTGRES_DB=your_db_name

# Azure OpenAI API Keys and Endpoints
AZURE_API_KEY_CENTRAL_US=your_azure_api_key_central_us
AZURE_API_BASE_CENTRAL_US=https://your-resource-name.openai.azure.com/

AZURE_API_KEY_AUSTRALIA_EAST=your_azure_api_key_australia_east
AZURE_API_BASE_AUSTRALIA_EAST=https://your-resource-name.openai.azure.com/

AZURE_API_KEY_EAST_US_2=your_azure_api_key_east_us_2
AZURE_API_BASE_EAST_US_2=https://your-resource-name.openai.azure.com/

AZURE_API_KEY_SOUTH_INDIA=your_azure_api_key_south_india
AZURE_API_BASE_SOUTH_INDIA=https://your-resource-name.openai.azure.com/

# Anthropic Claude API Key
ANTHROPIC_API_KEY=sk-ant-your-anthropic-api-key-here

# Bing Search API Key
BING_SEARCH_API_KEY=your_bing_search_api_key

# Optional: Logging level
LITELLM_LOG=INFO 